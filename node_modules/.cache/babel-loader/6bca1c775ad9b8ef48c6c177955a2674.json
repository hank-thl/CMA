{"ast":null,"code":"import axios from \"axios\"; // import $ from \"jquery\";\n\nexport default {\n  name: \"MySession\",\n\n  data() {\n    return {\n      myIndex: 0,\n      products: [],\n      input: {\n        productCatalog: \"3C\"\n      }\n    };\n  },\n\n  props: {\n    msg: String\n  },\n\n  mounted() {\n    axios.get(`http://bosian.ddns.net:8080/IIIFinalServlet_war_exploded/ProductServlet?pdaction=Select1`).then(res => {\n      this.products = res.data;\n    });\n  },\n\n  methods: {\n    changeIndex(num) {\n      if (this.myIndex >= 0 && this.myIndex < this.typeProducts.length) this.myIndex += num;\n    },\n\n    changePage(page) {\n      this.myIndex = page;\n      page();\n    },\n\n    page(p) {\n      if (p == this.myIndex) {\n        return \"active\";\n      }\n    },\n\n    addToCart(id) {\n      axios.get(`http://localhost:8080/IIIFinalServlet_war_exploded/CartServlet?pdaction=addToCart&editProductId=${id}`);\n      console.log(\"+1\");\n    }\n\n  },\n  computed: {\n    typeProducts() {\n      if (this.msg === \"全部商品\") {\n        return this.products;\n      } else {\n        return this.products.filter(item => {\n          return item.productCatalog === this.msg;\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2IA,OAAOA,KAAP,MAAkB,OAAlB,C,CACA;;AAEA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,CADJ;MAELC,QAAQ,EAAE,EAFL;MAGLC,KAAK,EAAE;QAAEC,cAAc,EAAE;MAAlB;IAHF,CAAP;EAKD,CARY;;EASbC,KAAK,EAAE;IACLC,GAAG,EAAEC;EADA,CATM;;EAYbC,OAAO,GAAG;IACRV,KAAI,CACDW,GADH,CAEK,0FAFL,EAIGC,IAJH,CAISC,GAAD,IAAS;MACb,KAAKT,QAAL,GAAgBS,GAAG,CAACX,IAApB;IACD,CANH;EAOD,CApBY;;EAqBbY,OAAO,EAAE;IACPC,WAAW,CAACC,GAAD,EAAM;MACf,IAAI,KAAKb,OAAL,IAAgB,CAAhB,IAAqB,KAAKA,OAAL,GAAe,KAAKc,YAAL,CAAkBC,MAA1D,EACE,KAAKf,OAAL,IAAgBa,GAAhB;IACH,CAJM;;IAKPG,UAAU,CAACC,IAAD,EAAO;MACf,KAAKjB,OAAL,GAAeiB,IAAf;MACAA,IAAI;IACL,CARM;;IASPA,IAAI,CAACC,CAAD,EAAI;MACN,IAAIA,KAAK,KAAKlB,OAAd,EAAuB;QACrB,OAAO,QAAP;MACF;IACD,CAbM;;IAcPmB,SAAS,CAACC,EAAD,EAAK;MACZvB,KAAK,CAACW,GAAN,CACG,mGAAkGY,EAAG,EADxG;MAGAC,OAAO,CAACC,GAAR,CAAY,IAAZ;IACD;;EAnBM,CArBI;EA0CbC,QAAQ,EAAE;IACRT,YAAY,GAAG;MACb,IAAI,KAAKT,GAAL,KAAa,MAAjB,EAAyB;QACvB,OAAO,KAAKJ,QAAZ;MACF,CAFA,MAEO;QACL,OAAO,KAAKA,QAAL,CAAcuB,MAAd,CAAsBC,IAAD,IAAU;UACpC,OAAOA,IAAI,CAACtB,cAAL,KAAwB,KAAKE,GAApC;QACD,CAFM,CAAP;MAGF;IACD;;EATO;AA1CG,CAAf","names":["axios","name","data","myIndex","products","input","productCatalog","props","msg","String","mounted","get","then","res","methods","changeIndex","num","typeProducts","length","changePage","page","p","addToCart","id","console","log","computed","filter","item"],"sourceRoot":"","sources":["/Users/hank/Desktop/Vue/Vue3/hello-world/src/components/MySession.vue"],"sourcesContent":["<template>\n  <section class=\"main-container col2-left-layout\">\n    <div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-sm-12\">\n          <div class=\"category-product\">\n            <div class=\"navbar nav-menu\">\n              <div class=\"navbar-collapse\">\n                <div class=\"new_title\">\n                  <h2>{{ msg }}</h2>\n                </div>\n              </div>\n              <!-- /.navbar-collapse -->\n            </div>\n            <div class=\"product-bestseller\">\n              <div class=\"product-bestseller-content\">\n                <div class=\"product-bestseller-list\">\n                  <div class=\"tab-container\">\n                    <!-- tab product -->\n                    <div class=\"tab-panel active\" id=\"tab-1\">\n                      <div class=\"category-products\">\n                        <ul class=\"products-grid\">\n                          <template v-for=\"(item, index) in typeProducts\">\n                            <li\n                              v-if=\"index >= myIndex && index < myIndex + 8\"\n                              :key=\"item.productId\"\n                              class=\"item col-lg-3 col-md-3 col-sm-4 col-xs-6\"\n                            >\n                              <div class=\"item-inner\">\n                                <div class=\"item-img\">\n                                  <div class=\"item-img-info\">\n                                    <router-link\n                                      class=\"product-image\"\n                                      title=\"Lorem ipsum dolor\"\n                                      :to=\"{\n                                        name: 'product',\n                                        params: { id: item.productId },\n                                      }\"\n                                    >\n                                      <img\n                                        class=\"productImage\"\n                                        alt=\"Lorem ipsum dolor\"\n                                        :src=\"item.productImg1\"\n                                      />\n                                    </router-link>\n                                  </div>\n                                </div>\n                                <div class=\"item-info\">\n                                  <div class=\"info-inner\">\n                                    <div class=\"item-title\">\n                                      <a\n                                        title=\"Lorem ipsum dolor\"\n                                        :href=\"products.productId\"\n                                        >{{ item.productName }}</a\n                                      >\n                                    </div>\n                                    <div class=\"item-content\">\n                                      <div class=\"rating\">\n                                        <div class=\"ratings\">\n                                          <div class=\"rating-box\">\n                                            <div class=\"rating width80\"></div>\n                                          </div>\n                                          <p class=\"rating-links\">\n                                            <a href=\"#\">1 Review(s)</a\n                                            ><span class=\"separator\">|</span\n                                            ><a href=\"#\">Add Review</a>\n                                          </p>\n                                        </div>\n                                      </div>\n                                      <div class=\"item-price\">\n                                        <div class=\"price-box\">\n                                          <span class=\"regular-price\"\n                                            ><span class=\"price\"\n                                              >${{ item.productPrice }}</span\n                                            ></span\n                                          >\n                                        </div>\n                                      </div>\n                                      <div class=\"action\">\n                                        <button\n                                          @click=\"addToCart(item.productId)\"\n                                          class=\"button btn-cart\"\n                                          type=\"button\"\n                                          data-original-title=\"Add to Cart\"\n                                        >\n                                          <span>Add to Cart</span>\n                                        </button>\n                                      </div>\n                                    </div>\n                                  </div>\n                                </div>\n                              </div>\n                            </li>\n                          </template>\n                        </ul>\n                      </div>\n                    </div>\n                    <!-- tab product -->\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n          <div class=\"col-sm-12\">\n            <div class=\"pagination\">\n              <ul>\n                <li>\n                  <a v-if=\"myIndex > 0\" @click=\"changeIndex(-8)\">&laquo;</a>\n                </li>\n                <template v-for=\"(item, index) in typeProducts\">\n                  <li\n                    :key=\"item.id\"\n                    v-if=\"index % 8 == 0 && typeProducts.length > 8\"\n                  >\n                    <a :class=\"page(index)\" @click=\"changePage(index)\">\n                      {{ index / 8 + 1 }}</a\n                    >\n                  </li>\n                </template>\n\n                <li>\n                  <a\n                    v-if=\"myIndex + 8 < typeProducts.length\"\n                    @click=\"changeIndex(+8)\"\n                    >&raquo;</a\n                  >\n                </li>\n              </ul>\n            </div>\n          </div>\n        </div>\n        <!-- ok -->\n      </div>\n      <!-- ok -->\n    </div>\n  </section>\n</template>\n\n<script>\nimport axios from \"axios\";\n// import $ from \"jquery\";\n\nexport default {\n  name: \"MySession\",\n  data() {\n    return {\n      myIndex: 0,\n      products: [],\n      input: { productCatalog: \"3C\" },\n    };\n  },\n  props: {\n    msg: String,\n  },\n  mounted() {\n    axios\n      .get(\n        `http://bosian.ddns.net:8080/IIIFinalServlet_war_exploded/ProductServlet?pdaction=Select1`\n      )\n      .then((res) => {\n        this.products = res.data;\n      });\n  },\n  methods: {\n    changeIndex(num) {\n      if (this.myIndex >= 0 && this.myIndex < this.typeProducts.length)\n        this.myIndex += num;\n    },\n    changePage(page) {\n      this.myIndex = page;\n      page();\n    },\n    page(p) {\n      if (p == this.myIndex) {\n        return \"active\";\n      }\n    },\n    addToCart(id) {\n      axios.get(\n        `http://localhost:8080/IIIFinalServlet_war_exploded/CartServlet?pdaction=addToCart&editProductId=${id}`\n      );\n      console.log(\"+1\");\n    },\n  },\n  computed: {\n    typeProducts() {\n      if (this.msg === \"全部商品\") {\n        return this.products;\n      } else {\n        return this.products.filter((item) => {\n          return item.productCatalog === this.msg;\n        });\n      }\n    },\n  },\n};\n</script>\n\n\n<style scoped>\n.pagination {\n  display: flex;\n  justify-content: center;\n}\n.pagination ul {\n  padding: 0;\n}\n.pagination li {\n  list-style-type: none;\n  display: inline;\n}\n\n.productImage {\n  aspect-ratio: 1/1;\n  object-fit: contain;\n}\n/* Pagination links */\n.pagination a {\n  color: black;\n  float: left;\n  padding: 8px 16px;\n  text-decoration: none;\n  transition: background-color 0.3s;\n}\n\n/* Style the active/current link */\n.pagination a.active {\n  background-color: #62a83d;\n  color: white;\n}\n\n/* Add a grey background color on mouse-over */\n.pagination a:hover:not(.active) {\n  background-color: #ddd;\n}\n</style>"]},"metadata":{},"sourceType":"module"}