{"ast":null,"code":"// import store from \"@/store/index\";\nimport $ from \"jquery\";\nimport axios from \"axios\";\nimport store from \"@/store/index\";\nexport default {\n  name: \"MyHeader\",\n\n  data() {\n    return {\n      cart: [],\n      amount: 0\n    };\n  },\n\n  mounted() {},\n\n  computed: {},\n  methods: {\n    dropdown() {\n      $(\".top-cart-content\").slideDown();\n    },\n\n    up() {\n      $(\".top-cart-content\").slideUp();\n    },\n\n    getCart() {\n      axios.get(`http://localhost:8081/api/cart/checkout`).then(res => {\n        this.cart = res.data;\n        this.amount = 0;\n\n        if (this.cart != \"購物車內目前沒有商品!\") {\n          $.each(this.cart, function (index, products) {\n            this.amount += parseInt(products.singleTotal);\n          });\n        }\n      });\n      axios.get(\"http://localhost:8081/api/test/session/mid\").then(res => {\n        if (res.status != 404) {\n          store.state.memberId = res.data;\n          console.log(store.state.memberId);\n        }\n      }).catch();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4KA;AACA,OAAOA,CAAP,MAAc,QAAd;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,KAAP,MAAkB,eAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EADD;MAELC,MAAM,EAAE;IAFH,CAAP;EAID,CAPY;;EAQbC,OAAO,GAAG,CAAE,CARC;;EASbC,QAAQ,EAAE,EATG;EAWbC,OAAO,EAAE;IACPC,QAAQ,GAAG;MACTV,CAAC,CAAC,mBAAD,CAAD,CAAuBW,SAAvB;IACD,CAHM;;IAIPC,EAAE,GAAG;MACHZ,CAAC,CAAC,mBAAD,CAAD,CAAuBa,OAAvB;IACD,CANM;;IAOPC,OAAO,GAAG;MACRb,KAAK,CAACc,GAAN,CAAW,yCAAX,EAAqDC,IAArD,CAA2DC,GAAD,IAAS;QACjE,KAAKZ,IAAL,GAAYY,GAAG,CAACb,IAAhB;QACA,KAAKE,MAAL,GAAc,CAAd;;QACA,IAAI,KAAKD,IAAL,IAAa,aAAjB,EAAgC;UAC9BL,CAAC,CAACkB,IAAF,CAAO,KAAKb,IAAZ,EAAkB,UAAUc,KAAV,EAAiBC,QAAjB,EAA2B;YAC3C,KAAKd,MAAL,IAAee,QAAQ,CAACD,QAAQ,CAACE,WAAV,CAAvB;UACD,CAFD;QAGF;MACD,CARD;MAUArB,KAAI,CACDc,GADH,CACO,4CADP,EAEGC,IAFH,CAESC,GAAD,IAAS;QACb,IAAIA,GAAG,CAACM,MAAJ,IAAc,GAAlB,EAAuB;UACrBrB,KAAK,CAACsB,KAAN,CAAYC,QAAZ,GAAuBR,GAAG,CAACb,IAA3B;UACAsB,OAAO,CAACC,GAAR,CAAYzB,KAAK,CAACsB,KAAN,CAAYC,QAAxB;QACF;MACD,CAPH,EAQGG,KARH;IASD;;EA3BM;AAXI,CAAf","names":["$","axios","store","name","data","cart","amount","mounted","computed","methods","dropdown","slideDown","up","slideUp","getCart","get","then","res","each","index","products","parseInt","singleTotal","status","state","memberId","console","log","catch"],"sourceRoot":"","sources":["/Users/hank/Desktop/Vue/Vue3/hello-world/src/components/MyHeader.vue"],"sourcesContent":["<template>\n  <header>\n    <div class=\"header-container\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <!-- Search-col -->\n          <div class=\"col-lg-4 col-xs-12 col-sm-4 search-section hidden-xs\">\n            <div class=\"search-box pull-left\">\n              <form\n                action=\"cat\"\n                method=\"POST\"\n                id=\"search_mini_form\"\n                name=\"Categories\"\n              >\n                <input\n                  type=\"text\"\n                  name=\"search\"\n                  value=\"\"\n                  placeholder=\"Search\"\n                  class=\"searchbox\"\n                  id=\"search\"\n                  autocomplete=\"off\"\n                /><button type=\"button\" class=\"search-btn-bg\">\n                  <span class=\"glyphicon glyphicon-search\"></span>&nbsp;\n                </button>\n              </form>\n            </div>\n          </div>\n          <!-- End Search-col -->\n          <div class=\"col-lg-4 col-sm-4 col-xs-12 logo-block\">\n            <!-- Header Logo -->\n            <div class=\"logo\">\n              <a href=\"index.html\">\n                <router-link\n                  :to=\"{\n                    name: 'home',\n                  }\"\n                >\n                  <img src=\"../assets/images/5.png\" alt=\"Kabrio logo\" />\n                </router-link>\n              </a>\n            </div>\n            <!-- End Header Logo -->\n          </div>\n          <div class=\"col-lg-4 col-sm-4 col-xs-12 toplinks-right\">\n            <!-- <div class=\"mm-toggle-wrap\">\n              <div class=\"mm-toggle\">\n                <i class=\"fa fa-align-justify\"></i\n                ><span class=\"mm-label\">Menu</span>\n              </div>\n            </div> -->\n\n            <div class=\"top-cart-contain\" @click.stop=\"dropdown(), getCart()\">\n              <!-- Top Cart -->\n              <div class=\"mini-cart\">\n                <div class=\"basket\">\n                  <a\n                    ><span>{{\n                      cart == \"購物車內目前沒有商品!\" ? \"0\" : cart.length\n                    }}</span></a\n                  >\n                </div>\n                <div>\n                  <div class=\"top-cart-content\" @mouseleave.stop=\"up()\">\n                    <div class=\"block-subtitle\">\n                      <div class=\"top-subtotal\">\n                        {{\n                          cart == \"購物車內目前沒有商品!\" ? \"0\" : cart.length\n                        }}\n                        items,\n\n                        <span class=\"price\">${{ amount }}</span>\n                      </div>\n                    </div>\n                    <ul\n                      v-if=\"cart != '購物車內目前沒有商品!'\"\n                      class=\"mini-products-list\"\n                      id=\"cart-sidebar\"\n                    >\n                      <li v-for=\"item in cart\" :key=\"item\" class=\"item\">\n                        <div class=\"item-inner\">\n                          <a\n                            class=\"product-image\"\n                            title=\"Lorem ipsum dolor\"\n                            href=\"#l\"\n                            ><img\n                              alt=\"Lorem ipsum dolor\"\n                              :src=\"item.productImg\"\n                          /></a>\n                          <div class=\"product-details\">\n                            <div class=\"access\">\n                              <a\n                                class=\"btn-remove1\"\n                                title=\"Remove This Item\"\n                                :href=\"\n                                  'http://localhost:8081/api/cart/remove/' +\n                                  item.productId\n                                \"\n                                >Remove</a\n                              ><a class=\"btn-edit\" title=\"Edit item\" href=\"#\"\n                                ><i class=\"icon-pencil\"></i\n                                ><span class=\"hidden\">Edit item</span></a\n                              >\n                            </div>\n                            <p class=\"product-name\">\n                              <a href=\"#\">{{ item.productName }}</a>\n                            </p>\n                            <strong>{{ item.qty }}</strong\n                            >x\n                            <span class=\"price\">{{ item.singleTotal }}</span>\n                          </div>\n                        </div>\n                      </li>\n                    </ul>\n                    <div class=\"actions\">\n                      <router-link\n                        :to=\"{\n                          name: memberId > 0 ? 'checkout' : 'login',\n                        }\"\n                      >\n                        <button\n                          class=\"btn-checkout\"\n                          title=\"Checkout\"\n                          type=\"button\"\n                        >\n                          <span>結帳</span>\n                        </button>\n                      </router-link>\n                    </div>\n                  </div>\n                </div>\n              </div>\n              <!-- End Top Cart -->\n            </div>\n            <!-- Header Top Links -->\n            <div class=\"toplinks\">\n              <div class=\"click-nav\">\n                <div class=\"no-js\">\n                  <a title=\"Company\" class=\"clicker\"\n                    ><span class=\"icon-account\"\n                      ><img\n                        src=\"../assets/images/icon-account.jpg\"\n                        alt=\"icon-account\" /></span\n                  ></a>\n                  <div class=\"top-links\">\n                    <ul class=\"links\">\n                      <li>\n                        <router-link\n                          :to=\"{\n                            name: 'login',\n                          }\"\n                        >\n                          登入\n                        </router-link>\n                      </li>\n                      <li><a href=\"login.html\">我的帳戶</a></li>\n                    </ul>\n                  </div>\n                </div>\n              </div>\n            </div>\n            <!-- End Header Top Links -->\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- <template v-for=\"item in cart\" :key=\"item.id\"> -->\n    <!-- </template> -->\n  </header>\n</template>\n\n<script>\n// import store from \"@/store/index\";\nimport $ from \"jquery\";\nimport axios from \"axios\";\nimport store from \"@/store/index\";\n\nexport default {\n  name: \"MyHeader\",\n  data() {\n    return {\n      cart: [],\n      amount: 0,\n    };\n  },\n  mounted() {},\n  computed: {},\n\n  methods: {\n    dropdown() {\n      $(\".top-cart-content\").slideDown();\n    },\n    up() {\n      $(\".top-cart-content\").slideUp();\n    },\n    getCart() {\n      axios.get(`http://localhost:8081/api/cart/checkout`).then((res) => {\n        this.cart = res.data;\n        this.amount = 0;\n        if (this.cart != \"購物車內目前沒有商品!\") {\n          $.each(this.cart, function (index, products) {\n            this.amount += parseInt(products.singleTotal);\n          });\n        }\n      });\n\n      axios\n        .get(\"http://localhost:8081/api/test/session/mid\")\n        .then((res) => {\n          if (res.status != 404) {\n            store.state.memberId = res.data;\n            console.log(store.state.memberId);\n          }\n        })\n        .catch();\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.cart {\n  width: 300px !important;\n  padding: 0 !important;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}