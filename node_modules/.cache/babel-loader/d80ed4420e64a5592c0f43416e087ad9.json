\"off\"\n                /><button type=\"button\" class=\"search-btn-bg\">\n                  <span class=\"glyphicon glyphicon-search\"></span>&nbsp;\n                </button>\n              </form>\n            </div>\n          </div>\n          <!-- End Search-col -->\n          <div class=\"col-lg-4 col-sm-4 col-xs-12 logo-block\">\n            <!-- Header Logo -->\n            <div class=\"logo\">\n              <a href=\"index.html\">\n                <router-link\n                  :to=\"{\n                    name: 'home',\n                  }\"\n                >\n                  <img src=\"../assets/images/5.png\" alt=\"Kabrio logo\" />\n                </router-link>\n              </a>\n            </div>\n            <!-- End Header Logo -->\n          </div>\n          <div class=\"col-lg-4 col-sm-4 col-xs-12 toplinks-right\">\n            <!-- <div class=\"mm-toggle-wrap\">\n              <div class=\"mm-toggle\">\n                <i class=\"fa fa-align-justify\"></i\n                ><span class=\"mm-label\">Menu</span>\n              </div>\n            </div> -->\n\n            <div\n              class=\"top-cart-contain\"\n              @click=\"dropdown()\"\n              @mouseenter=\"dropdown()\"\n            >\n              <!-- Top Cart -->\n              <div class=\"mini-cart\">\n                <div class=\"basket\">\n                  <a><span>3</span></a>\n                </div>\n                <div>\n                  <div class=\"top-cart-content\" @mouseleave=\"up()\">\n                    <div class=\"block-subtitle\">\n                      <div class=\"top-subtotal\">\n                        {{ cart.length }} items,\n\n                        <span class=\"price\">${{ amount }}</span>\n                      </div>\n                    </div>\n                    <ul class=\"mini-products-list\" id=\"cart-sidebar\">\n                      <li class=\"item\">\n                        <div class=\"item-inner\">\n                          <a\n                            class=\"product-image\"\n                            title=\"Lorem ipsum dolor\"\n                            href=\"#l\"\n                            ><img\n                              alt=\"Lorem ipsum dolor\"\n                              src=\"@/assets/products-images/product1.jpg\"\n                          /></a>\n                          <div class=\"product-details\">\n                            <div class=\"access\">\n                              <a\n                                class=\"btn-remove1\"\n                                title=\"Remove This Item\"\n                                href=\"#\"\n                                >Remove</a\n                              ><a class=\"btn-edit\" title=\"Edit item\" href=\"#\"\n                                ><i class=\"icon-pencil\"></i\n                                ><span class=\"hidden\">Edit item</span></a\n                              >\n                            </div>\n                            <p class=\"product-name\">\n                              <a href=\"#\">Lorem ipsum dolor</a>\n                            </p>\n                            <strong>1</strong>x\n                            <span class=\"price\">$79.99</span>\n                          </div>\n                        </div>\n                      </li>\n                    </ul>\n                    <div class=\"actions\">\n                      <button\n                        class=\"btn-checkout\"\n                        title=\"Checkout\"\n                        type=\"button\"\n                        onclick=\"location.href='checkout.html';\"\n                      >\n                        <span>結帳</span>\n                      </button>\n                      <a href=\"shopping_cart.html\" class=\"view-cart\">\n                        <span>購物車</span>\n                      </a>\n                    </div>\n                  </div>\n                </div>\n              </div>\n              <!-- End Top Cart -->\n            </div>\n            <!-- Header Top Links -->\n            <div class=\"toplinks\">\n              <div class=\"click-nav\">\n                <div class=\"no-js\">\n                  <a title=\"Company\" class=\"clicker\"\n                    ><span class=\"icon-account\"\n                      ><img\n                        src=\"../assets/images/icon-account.jpg\"\n                        alt=\"icon-account\" /></span\n                  ></a>\n                  <div class=\"top-links\">\n                    <ul class=\"links\">\n                      <li><a href=\"login.html\">登入</a></li>\n                      <li><a href=\"login.html\">我的帳戶</a></li>\n                    </ul>\n                  </div>\n                </div>\n              </div>\n            </div>\n            <!-- End Header Top Links -->\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- <template v-for=\"item in cart\" :key=\"item.id\"> -->\n    <!-- </template> -->\n  </header>\n</template>\n\n<script>\nimport store from \"@/store/index\";\nimport $ from \"jquery\";\nimport { ref } from \"vue\";\n\nexport default {\n  name: \"MyHeader\",\n\n  mounted() {\n    store.dispatch(\"GETCART\");\n    // store.state.cart.foreach((elm) => {\n    //   amount.value += elm.productPrice;\n    // });\n  },\n  computed: {\n    cart() {\n      return store.state.cart;\n    },\n  },\n  methods: {\n    dropdown() {\n      $(\".top-cart-content\").slideDown();\n    },\n    up() {\n      $(\".top-cart-content\").slideUp();\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.cart {\n  width: 300px !important;\n  padding: 0 !important;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}