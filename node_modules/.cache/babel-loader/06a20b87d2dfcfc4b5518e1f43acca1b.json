  <div class=\"display-product-option\">\n                <div class=\"col-lg-3 col-md-10\">\n                  <div id=\"sort-by\">\n                    <ul>\n                      <li>\n                        篩 選:<span class=\"right-arrow\"></span>\n                        <ul>\n                          <li><a @click=\"priceH2L()\">價格:高➜低</a></li>\n                          <li><a @click=\"priceL2H()\">價格:低➜高</a></li>\n                        </ul>\n                      </li>\n                    </ul>\n                  </div>\n                </div>\n              </div>\n            </div>\n\n            <!-- 右邊下面 -->\n\n            <!-- 右邊中間 -->\n            <div class=\"category-products\">\n              <ul class=\"products-grid\" id=\"test1\"></ul>\n            </div>\n            <!-- 右邊下面 -->\n          </article>\n          <!--  End article -->\n        </div>\n        <!-- 左邊目錄 -->\n        <div class=\"col-left sidebar col-sm-3 col-xs-12 col-sm-pull-9\">\n          <aside class=\"col-left sidebar\">\n            <div class=\"side-nav-categories\">\n              <div class=\"block-title\">目錄</div>\n              <div class=\"box-content box-category\">\n                <ul>\n                  <li class=\"mega-menu\"><a href=\"grid.html\">飾品</a></li>\n                  <li class=\"mega-menu\"><a href=\"grid2.html\">質感燈飾</a></li>\n                  <li class=\"mega-menu\"><a href=\"grid.html\">服飾</a></li>\n                  <li class=\"mega-menu\"><a href=\"grid.html\">日常用品</a></li>\n                  <li class=\"mega-menu\"><a href=\"grid.html\">居家收納</a></li>\n                  <li class=\"mega-menu\"><a href=\"grid.html\">外出旅行</a></li>\n                </ul>\n              </div>\n              <!--box-content box-category-->\n            </div>\n            <div class=\"hot-banner\">\n              <!-- <img alt=\"banner\" src=\"colinimage/rakuten.png\" /> -->\n            </div>\n            <div class=\"block block-layered-nav\">\n              <div class=\"block-title\">\n                <!-- <img\n                  src=\"colinimage/sagasu.jfif\"\n                  width=\"20\"\n                  height=\"30\"\n                /> -->\n                條件篩選\n              </div>\n\n              <div class=\"block-content\">\n                <p class=\"block-subtitle\"></p>\n                <div class=\"price-filter\" style=\"text-align: center\">\n                  <label><strong>價格</strong></label>\n                  <ul>\n                    <!-- <li><input  class=\"custom-control-input\" id=\"below-50\"><label\n                                                    class=\"custom-control-label\" for=\"below-20\">below-$50</label></li> -->\n                    <li style=\"text-align: center\">\n                      <input\n                        type=\"number\"\n                        style=\"width: 80px\"\n                        maxlength=\"5\"\n                        min=\"0\"\n                        step=\"100\"\n                        placeholder=\"輸入金額\"\n                        id=\"inputMi\"\n                      />\n                      <b>~</b>\n                      <input\n                        type=\"number\"\n                        style=\"width: 80px\"\n                        maxlength=\"5\"\n                        min=\"0\"\n                        step=\"100\"\n                        placeholder=\"輸入金額\"\n                        id=\"inputMa\"\n                      />\n                    </li>\n                  </ul>\n\n                  <ul>\n                    <li></li>\n                    <br />\n                    <li style=\"text-align: center\">\n                      <input\n                        type=\"button\"\n                        value=\"清除全部\"\n                        @click=\"clearText(), setList()\"\n                      /><br />\n                    </li>\n                    <li></li>\n                    <br />\n                    <li>\n                      <input\n                        type=\"button\"\n                        value=\"套用條件\"\n                        @click=\"doText()\"\n                      /><br />\n                    </li>\n                  </ul>\n                </div>\n              </div>\n            </div>\n          </aside>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\n// import store from \"@/store/index\";\n// import axios from \"axios\";\n\nimport $ from \"jquery\";\n\nexport default {\n  name: \"MyGridV2\",\n  data() {\n    return {\n      myIndex: 0,\n      sortState: 0,\n      categories: [\"全部商品\", \"飾品\", \"家飾佈置\", \"3C\", \"運動用品\"],\n      category: 0,\n    };\n  },\n\n  mounted() {\n    this.category = parseInt(this.$route.params.category);\n    this.getList;\n  },\n  computed: {\n    getList() {\n      return this.setList();\n    },\n  },\n  methods: {\n    setList() {\n      $(\"#test1\").empty();\n      let bestSaleList;\n      let categories = [\"全部商品\", \"飾品\", \"家飾佈置\", \"3C\", \"運動用品\"];\n      let categoryId = this.category;\n      $.get(\"http://localhost:8081/api/products/\", function (e) {\n        bestSaleList = e;\n        $.each(bestSaleList, (index, products) => {\n          // console.log(index);\n          // console.log(this.category);\n          if (\"全部商品\" == categories[categoryId]) {\n            $(\"#test1\")\n              .append(` <li class=\"item col-lg-4 col-md-4 col-sm-4 col-xs-6\">\n                                        <div class=\"item-inner\">\n                                            <div class=\"item-img\">\n                                                <div class=\"item-img-info\"><a href=\"#/product/${products.productId}\" title=\"Lorem ipsum dolor\"\n                                                        class=\"product-image\"><img src=\"${products.productImg1}\"\n                                                            alt=\"Lorem ipsum dolor\" style=\"aspect-ratio: 1/1;\"></a>\n 