cts: [],\n    };\n  },\n  mounted() {\n    axios\n      .get(\n        \"http://bosian.ddns.net:8080/IIIFinalServlet_war_exploded/ProductServlet?pdaction=Select1\"\n      )\n      .then((res) => {\n        this.products = res.data;\n      });\n  },\n};\n\nconsole.log(\"Mysession\");\n</script>"]},"metadata":{},"sourceType":"module"}