{"ast":null,"code":"import axios from \"axios\"; // import $ from \"jquery\";\n\nimport store from \"@/store/index\";\nimport Swal from \"sweetalert2\";\nexport default {\n  name: \"MySession\",\n\n  data() {\n    return {\n      myIndex: 0,\n      products: []\n    };\n  },\n\n  props: {\n    msg: String\n  },\n\n  mounted() {\n    axios.get(`http://localhost:8081/api/products/`).then(res => {\n      this.products = res.data;\n    });\n  },\n\n  methods: {\n    changeIndex(num) {\n      if (this.myIndex >= 0 && this.myIndex < this.typeProducts.length) this.myIndex += num;\n    },\n\n    changePage(page) {\n      this.myIndex = page;\n    },\n\n    addToCart(id) {\n      axios.get(`http://localhost:8081/api/cart/add/${id}/qty/`);\n      store.state.cartAmount += 1;\n      Swal.fire(\"CMA\", \"成功加入購物車\", \"success\");\n    }\n\n  },\n  computed: {\n    typeProducts() {\n      if (this.msg === \"全部商品\") {\n        return this.products;\n      } else {\n        return this.products.filter(item => {\n          return item.productCatalog === this.msg;\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiJA,OAAOA,KAAP,MAAkB,OAAlB,C,CACA;;AACA,OAAOC,KAAP,MAAkB,eAAlB;AACA,OAAOC,IAAP,MAAiB,aAAjB;AAEA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,CADJ;MAELC,QAAQ,EAAE;IAFL,CAAP;EAID,CAPY;;EAQbC,KAAK,EAAE;IACLC,GAAG,EAAEC;EADA,CARM;;EAWbC,OAAO,GAAG;IACRV,KAAK,CAACW,GAAN,CAAW,qCAAX,EAAiDC,IAAjD,CAAuDC,GAAD,IAAS;MAC7D,KAAKP,QAAL,GAAgBO,GAAG,CAACT,IAApB;IACD,CAFD;EAGD,CAfY;;EAgBbU,OAAO,EAAE;IACPC,WAAW,CAACC,GAAD,EAAM;MACf,IAAI,KAAKX,OAAL,IAAgB,CAAhB,IAAqB,KAAKA,OAAL,GAAe,KAAKY,YAAL,CAAkBC,MAA1D,EACE,KAAKb,OAAL,IAAgBW,GAAhB;IACH,CAJM;;IAKPG,UAAU,CAACC,IAAD,EAAO;MACf,KAAKf,OAAL,GAAee,IAAf;IACD,CAPM;;IASPC,SAAS,CAACC,EAAD,EAAK;MACZtB,KAAK,CAACW,GAAN,CAAW,sCAAqCW,EAAG,OAAnD;MACArB,KAAK,CAACsB,KAAN,CAAYC,UAAZ,IAA0B,CAA1B;MACAtB,IAAI,CAACuB,IAAL,CAAU,KAAV,EAAiB,SAAjB,EAA4B,SAA5B;IACD;;EAbM,CAhBI;EA+BbC,QAAQ,EAAE;IACRT,YAAY,GAAG;MACb,IAAI,KAAKT,GAAL,KAAa,MAAjB,EAAyB;QACvB,OAAO,KAAKF,QAAZ;MACF,CAFA,MAEO;QACL,OAAO,KAAKA,QAAL,CAAcqB,MAAd,CAAsBC,IAAD,IAAU;UACpC,OAAOA,IAAI,CAACC,cAAL,KAAwB,KAAKrB,GAApC;QACD,CAFM,CAAP;MAGF;IACD;;EATO;AA/BG,CAAf","names":["axios","store","Swal","name","data","myIndex","products","props","msg","String","mounted","get","then","res","methods","changeIndex","num","typeProducts","length","changePage","page","addToCart","id","state","cartAmount","fire","computed","filter","item","productCatalog"],"sourceRoot":"","sources":["/Users/hank/Desktop/Vue/Vue3/hello-world/src/components/MySession.vue"],"sourcesContent":["<template>\n  <section class=\"main-container col2-left-layout\">\n    <div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-sm-12\">\n          <div class=\"category-product\">\n            <div class=\"navbar nav-menu\">\n              <div class=\"navbar-collapse\">\n                <div class=\"new_title\">\n                  <h2>{{ msg }}</h2>\n                </div>\n              </div>\n              <!-- /.navbar-collapse -->\n            </div>\n            <div class=\"product-bestseller\">\n              <div class=\"product-bestseller-content\">\n                <div class=\"product-bestseller-list\">\n                  <div class=\"tab-container\">\n                    <!-- tab product -->\n                    <div class=\"tab-panel active\" id=\"tab-1\">\n                      <div class=\"category-products\">\n                        <ul class=\"products-grid\">\n                          <template v-for=\"(item, index) in typeProducts\">\n                            <li\n                              v-if=\"index >= myIndex && index < myIndex + 8\"\n                              :key=\"item.productId\"\n                              class=\"item col-lg-3 col-md-3 col-sm-4 col-xs-6\"\n                            >\n                              <div class=\"item-inner\">\n                                <div class=\"item-img\">\n                                  <div class=\"item-img-info\">\n                                    <router-link\n                                      class=\"product-image\"\n                                      title=\"Lorem ipsum dolor\"\n                                      :to=\"{\n                                        name: 'product',\n                                        params: { id: item.productId },\n                                      }\"\n                                    >\n                                      <img\n                                        class=\"productImage\"\n                                        alt=\"Lorem ipsum dolor\"\n                                        :src=\"item.productImg1\"\n                                      />\n                                    </router-link>\n                                  </div>\n                                </div>\n                                <div class=\"item-info\">\n                                  <div class=\"info-inner\">\n                                    <div class=\"item-title\">\n                                      <router-link\n                                        title=\"productname\"\n                                        :to=\"{\n                                          name: 'product',\n                                          params: { id: item.productId },\n                                        }\"\n                                        >{{ item.productName }}</router-link\n                                      >\n                                    </div>\n                                    <div class=\"item-content\">\n                                      <!-- <div class=\"rating\">\n                                        <div class=\"ratings\">\n                                          <div class=\"rating-box\">\n                                            <div class=\"rating width80\"></div>\n                                          </div>\n                                          <p class=\"rating-links\">\n                                            <a href=\"#\">1 Review(s)</a\n                                            ><span class=\"separator\">|</span\n                                            ><a href=\"#\">Add Review</a>\n                                          </p>\n                                        </div>\n                                      </div> -->\n                                      <div class=\"item-price\">\n                                        <div class=\"price-box\">\n                                          <span class=\"regular-price\"\n                                            ><span class=\"price\"\n                                              >${{ item.productPrice }}</span\n                                            ></span\n                                          >\n                                        </div>\n                                      </div>\n                                      <div class=\"action\">\n                                        <button\n                                          @click=\"addToCart(item.productId)\"\n                                          class=\"button btn-cart\"\n                                          type=\"button\"\n                                          data-original-title=\"Add to Cart\"\n                                        >\n                                          <span>Add to Cart</span>\n                                        </button>\n                                      </div>\n                                    </div>\n                                  </div>\n                                </div>\n                              </div>\n                            </li>\n                          </template>\n                        </ul>\n                      </div>\n                    </div>\n                    <!-- tab product -->\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n          <div class=\"col-sm-12\">\n            <div class=\"pagination\">\n              <ul>\n                <li>\n                  <a v-if=\"myIndex > 0\" @click=\"changeIndex(-8)\">&laquo;</a>\n                </li>\n                <template v-for=\"(item, index) in typeProducts\">\n                  <li\n                    :key=\"item.id\"\n                    v-if=\"index % 8 == 0 && typeProducts.length > 8\"\n                  >\n                    <a\n                      :class=\"index == this.myIndex ? 'active' : ''\"\n                      @click=\"changePage(index)\"\n                    >\n                      {{ index / 8 + 1 }}</a\n                    >\n                  </li>\n                </template>\n\n                <li>\n                  <a\n                    v-if=\"myIndex + 8 < typeProducts.length\"\n                    @click=\"changeIndex(+8)\"\n                    >&raquo;</a\n                  >\n                </li>\n              </ul>\n            </div>\n          </div>\n        </div>\n        <!-- ok -->\n      </div>\n      <!-- ok -->\n    </div>\n  </section>\n</template>\n\n<script>\nimport axios from \"axios\";\n// import $ from \"jquery\";\nimport store from \"@/store/index\";\nimport Swal from \"sweetalert2\";\n\nexport default {\n  name: \"MySession\",\n  data() {\n    return {\n      myIndex: 0,\n      products: [],\n    };\n  },\n  props: {\n    msg: String,\n  },\n  mounted() {\n    axios.get(`http://localhost:8081/api/products/`).then((res) => {\n      this.products = res.data;\n    });\n  },\n  methods: {\n    changeIndex(num) {\n      if (this.myIndex >= 0 && this.myIndex < this.typeProducts.length)\n        this.myIndex += num;\n    },\n    changePage(page) {\n      this.myIndex = page;\n    },\n\n    addToCart(id) {\n      axios.get(`http://localhost:8081/api/cart/add/${id}/qty/`);\n      store.state.cartAmount += 1;\n      Swal.fire(\"CMA\", \"成功加入購物車\", \"success\");\n    },\n  },\n  computed: {\n    typeProducts() {\n      if (this.msg === \"全部商品\") {\n        return this.products;\n      } else {\n        return this.products.filter((item) => {\n          return item.productCatalog === this.msg;\n        });\n      }\n    },\n  },\n};\n</script>\n\n\n<style scoped>\n.pagination {\n  display: flex;\n  justify-content: center;\n}\n.pagination ul {\n  padding: 0;\n}\n.pagination li {\n  list-style-type: none;\n  display: inline;\n}\n\n.productImage {\n  aspect-ratio: 1/1;\n  object-fit: contain;\n}\n/* Pagination links */\n.pagination a {\n  color: black;\n  float: left;\n  padding: 8px 16px;\n  text-decoration: none;\n  transition: background-color 0.3s;\n}\n\n/* Style the active/current link */\n.pagination a.active {\n  background-color: #62a83d;\n  color: white;\n}\n\n/* Add a grey background color on mouse-over */\n.pagination a:hover:not(.active) {\n  background-color: #ddd;\n}\n</style>"]},"metadata":{},"sourceType":"module"}