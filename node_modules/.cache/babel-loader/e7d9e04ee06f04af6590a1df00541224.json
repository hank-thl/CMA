  <div class=\"row\">\n        <section>\n          <div class=\"col-main\">\n            <div class=\"dashboard\">\n              <div class=\"welcome-msg\" id=\"helloName\"></div>\n              <div class=\"recent-orders\">\n                <div class=\"title-buttons\">\n                  <strong>最近訂單</strong><a id=\"oldOder\">&nbsp;</a>\n                </div>\n                <div class=\"table-responsive\">\n                  <table class=\"data-table\" id=\"my-orders-table\">\n                    <thead>\n                      <tr class=\"first last\">\n                        <th>訂單編號 #</th>\n                        <th>訂單建立日期</th>\n                        <th>付款方式</th>\n                        <th>到貨超商</th>\n                        <th>宅配地址</th>\n                        <th><span class=\"nobr\">訂單總額</span></th>\n                        <th>狀態</th>\n                        <th>&nbsp;</th>\n                      </tr>\n                    </thead>\n                    <tbody id=\"naka\"></tbody>\n                  </table>\n                </div>\n              </div>\n              <div class=\"box-account\">\n                <div class=\"page-title\">\n                  <h1><strong>帳號 資訊</strong></h1>\n                </div>\n                <div class=\"col2-set\">\n                  <div class=\"col-1\">\n                    <h5>會員 資訊</h5>\n                    <a href=\"#\">編輯</a>\n                    <div id=\"memberinformation\">\n                      <!-- <p>John Doe<br> jone-doe@example.com<br><a href=\"#\">Change Password</a></p> -->\n                    </div>\n                  </div>\n                  <div class=\"col-2\"></div>\n                </div>\n                <div class=\"col2-set\">\n                  <h2><strong>通訊錄</strong></h2>\n                  <div class=\"col-1\">\n                    <h5>帳單 地址</h5>\n                    <div id=\"memberAdress\">\n                      <!-- <address>John Doe<br> USA<br> Anytown, Alabama, 46532<br> United States<br> T: 454541<br><a\n                          href=\"#\">Edit Address</a></address> -->\n                    </div>\n                  </div>\n                  <div class=\"col-2\"></div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </section>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport $ from \"jquery\";\nimport store from \"@/store/index\";\n\nexport default {\n  name: \"MyDashboard\",\n  mounted() {\n    let Names;\n    let Orders;\n    var settings = {\n      url: \"http://localhost:8081/api/test/session/mid\",\n      method: \"GET\",\n    };\n\n    $.ajax(settings).done(function (response) {\n      console.log(response);\n\n      var settings = {\n        url: `http://localhost:8081/api/member/${response}`,\n        method: \"GET\",\n        timeout: 0,\n      };\n      $.ajax(settings).done(function (e) {\n        console.log(e);\n\n        Names = JSON.parse(e);\n        console.log(\"MID=\" + Names.memberId);\n\n        $(\"#helloName\").append(\n          `<strong>你好,<h2> ${Names.memberFirstname} ${Names.memberLastname}</h2> </strong>`\n        );\n        $(\n          \"#memberinformation\"\n        ).append(` <p> 姓名: ${Names.memberFirstname} ${Names.memberLastname}<br> 信箱: ${Names.memberEmail}<br></p>\n`);\n        $(\"#memberAdress\").append(\n          ` <address>姓名: ${Names.memberFirstname} ${Names.memberLastname}<br> 地址: ${Names.memberAddr}<br> 電話: ${Names.memberTel} <br></address>`\n        );\n\n        //以下開始填入訂單資訊\n        var settings = {\n          url: \"http://localhost:8081/api/orders\",\n          method: \"POST\",\n          timeout: 0,\n          headers: {\n            \"Content-Type\": \"application/json\",\n          },\n          data: JSON.stringify({\n            orderId: \"\",\n            status: \"\",\n            memberId: Names.memberId,\n          }),\n        };\n        $.ajax(settings).done(function (e) {\n          Orders = JSON.parse(e);\n          console.log(Orders);\n          $.each(Orders, (index, products) => {\n            var m = Math.random() * 10000 + 200;\n            var radom = Math.floor(m);\n            var status = products.狀態;\n            switch (status) {\n              case \"01\":\n                status = \"成立\";\n                break;\n              case \"02\":\n                status = \"已付款\";\n                break;\n              case \"03\":\n                status = \"已出貨\";\n                break;\n              case \"031\":\n                status = \"未付款已出貨\";\n                break;\n              case \"04\":\n                status = \"取消\";\n                break;\n              case \"05\":\n                status = \"完成交付\";\n                break;\n              case \"06\":\n                status = \"退貨\";\n                break;\n            }\n            console.log(typeof status);\n            //   if(products.會員編號==n){\n            $(\"#naka\").append(` <tr >\n                <td>${products.訂單編號}</td>\n                <td>${products.訂單建立日期}</td>\n                <td>${products.付款方式}</td>\n                <td>${products.到貨超商}</td>\n                <td>${products.宅配地址}</td>\n                <td><span class=\"price\">$${radom}</span></td>\n                <td><em>${status}</em></td>\n                <td class=\"a-center last\"><span class=\"nobr\"></td>\n              </tr>`);\n          });\n        });\n        //填入訂單資訊完畢\n      });\n    });\n  },\n};\n</script>\n\n<style scoped>\n</style>"]},"metadata":{},"sourceType":"module"}