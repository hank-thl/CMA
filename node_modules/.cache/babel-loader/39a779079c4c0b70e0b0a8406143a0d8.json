        <li><a onclick=\"priceH2L()\">價格:高➜低</a></li>\n                        <li><a onclick=\"priceL2H()\">價格:低➜高</a></li>\n                        <li><a href=\"#\">Position</a></li>\n                      </ul>\n                    </li>\n                  </ul>\n                </div>\n              </div>\n            </div>\n            <!-- 右邊中間 -->\n            <div class=\"category-products\">\n              <ul class=\"products-grid\" id=\"test1\">\n                <template v-for=\"(item, index) in typeProducts\">\n                  <li\n                    v-if=\"index >= myIndex && index < myIndex + 8\"\n                    :key=\"item.id\"\n                    class=\"item col-lg-4 col-md-4 col-sm-4 col-xs-6\"\n                  >\n                    <div class=\"item-inner\">\n                      <div class=\"item-img\">\n                        <div class=\"item-img-info\">\n                          <a\n                            href=\"#\"\n                            title=\"Lorem ipsum dolor\"\n                            class=\"product-image\"\n                            ><img\n                              :src=\"item.productImg1\"\n                              alt=\"Lorem ipsum dolor\"\n                              class=\"productImage\"\n                          /></a>\n                        </div>\n                      </div>\n                      <div class=\"item-info\">\n                        <div class=\"info-inner\">\n                          <div class=\"item-title\">\n                            <a title=\"Lorem ipsum dolor\" href=\"#\">{{\n                              item.productName\n                            }}</a>\n                          </div>\n                          <div class=\"item-content\">\n                            <div class=\"rating\">\n                              <div class=\"ratings\">\n                                <div class=\"rating-box\">\n                                  <div class=\"rating width80\"></div>\n                                </div>\n                                <p class=\"rating-links\">\n                                  <a href=\"#\">1 Review(s)</a\n                                  ><span class=\"separator\">|</span\n                                  ><a href=\"#\">Add Review</a>\n                                </p>\n                              </div>\n                            </div>\n                            <div class=\"item-price\">\n                              <div class=\"price-box\">\n                                <p class=\"special-price\">\n                                  <span class=\"price-label\">Special Price</span\n                                  ><span class=\"price\"\n                                    >${{ item.productPrice }}</span\n                                  >\n                                </p>\n                              </div>\n                            </div>\n                            <div class=\"action\">\n                              <button\n                                class=\"button btn-cart\"\n                                type=\"button\"\n                                data-original-title=\"Add to Cart\"\n                              >\n                                <span>Add to Cart</span>\n                              </button>\n                            </div>\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                  </li>\n                </template>\n              </ul>\n            </div>\n            <!-- 右邊下面 -->\n          </article>\n          <!--  End article -->\n        </div>\n        <!-- 左邊目錄 -->\n        <div class=\"col-left sidebar col-sm-3 col-xs-12 col-sm-pull-9\">\n          <aside class=\"col-left sidebar\">\n            <div class=\"side-nav-categories\">\n              <div class=\"block-title\">目錄</div>\n              <div class=\"box