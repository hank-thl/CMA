"number\"\n                        style=\"width: 80px\"\n                        maxlength=\"5\"\n                        min=\"0\"\n                        step=\"100\"\n                        placeholder=\"輸入金額\"\n                        id=\"inputMa\"\n                      />\n                    </li>\n                  </ul>\n\n                  <ul>\n                    <li></li>\n                    <br />\n                    <li style=\"text-align: center\">\n                      <input\n                        type=\"button\"\n                        value=\"清除全部\"\n                        onclick=\"clearText()\"\n                      /><br />\n                    </li>\n                    <li></li>\n                    <br />\n                    <li>\n                      <input\n                        type=\"button\"\n                        value=\"套用條件\"\n                        onclick=\"doText()\"\n                      /><br />\n                    </li>\n                  </ul>\n                </div>\n              </div>\n            </div>\n          </aside>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nimport $ from \"jquery\";\n\nexport default {\n  name: \"MyGrid\",\n  data() {\n    return {\n      myIndex: 0,\n      products: [],\n      input: { productCatalog: \"3C\" },\n    };\n  },\n  props: {\n    msg: String,\n  },\n  mounted() {\n    axios\n      .get(\n        `http://bosian.ddns.net:8080/IIIFinalServlet_war_exploded/ProductServlet?pdaction=Select1`\n      )\n      .then((res) => {\n        this.products = res.data;\n      });\n  },\n  computed: {\n    typeProducts() {\n      if (this.msg === \"全部商品\") {\n        return this.products;\n      } else {\n        return this.products.filter((item) => {\n          return item.productCatalog === this.msg;\n        });\n      }\n    },\n  },\n};\n\nlet bestSaleList = [];\n\n$.get(\n  \"http://bosian.ddns.net:8080/I