 case \"03\":\n                status = \"已出貨\";\n                break;\n\n              case \"031\":\n                status = \"未付款已出貨\";\n                break;\n\n              case \"04\":\n                status = \"取消\";\n                break;\n\n              case \"05\":\n                status = \"完成交付\";\n                break;\n\n              case \"06\":\n                status = \"退貨\";\n                break;\n            }\n\n            console.log(typeof status); //   if(products.會員編號==n){\n\n            $(\"#naka\").append(` <tr >\n                <td>${products.訂單編號}</td>\n                <td>${products.訂單建立日期}</td>\n                <td>${products.付款方式}</td>\n                <td>${products.到貨超商}</td>\n                <td>${products.宅配地址}</td>\n                <td><span class=\"price\">$${radom}</span></td>\n                <td><em>${status}</em></td>\n                <td class=\"a-center last\"><span class=\"nobr\"></td>\n              </tr>`);\n          });\n        }); //填入訂單資訊完畢\n      });\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAiEA,OAAOA,CAAP,MAAc,QAAd;AACA,OAAOC,KAAP,MAAkB,eAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,aADO;;EAEbC,OAAO,GAAG;IACR,IAAIC,KAAJ;IACA,IAAIC,MAAJ;IACA,IAAIC,QAAO,GAAI;MACbC,GAAG,EAAE,4CADQ;MAEbC,MAAM,EAAE;IAFK,CAAf;IAKAR,CAAC,CAACS,IAAF,CAAOH,QAAP,EAAiBI,IAAjB,CAAsB,UAAUC,QAAV,EAAoB;MACxCC,OAAO,CAACC,GAAR,CAAYF,QAAZ;MAEA,IAAIL,QAAO,GAAI;QACbC,GAAG,EAAG,oCAAmCI,QAAS,EADrC;QAEbH,MAAM,EAAE,KAFK;QAGbM,OAAO,EAAE;MAHI,CAAf;MAKAd,CAAC,CAACS,IAAF,CAAOH,QAAP,EAAiBI,IAAjB,CAAsB,UAAUK,CAAV,EAAa;QACjCH,OAAO,CAACC,GAAR,CAAYE,CAAZ;QAEAX,KAAI,GAAIY,IAAI,CAACC,KAAL,CAAWF,CAAX,CAAR;QACAH,OAAO,CAACC,GAAR,CAAY,SAAST,KAAK,CAACc,QAA3B;QAEAlB,CAAC,CAAC,YAAD,CAAD,CAAgBmB,MAAhB,CACG,mBAAkBf,KAAK,CAACgB,eAAe,IAAIhB,KAAK,CAACiB,cAAe,iBADnE;QAGArB,CAAC,CACC,oBADD,CAAD,CAEEmB,MAFF,CAES,YAAYf,KAAK,CAACgB,eAAe,IAAIhB,KAAK,CAACiB,cAAe,YAAWjB,KAAK,CAACkB,WAAY;AACxG,CAHQ;QAIAtB,CAAC,CAAC,eAAD,CAAD,CAAmBmB,MAAnB,CACE,iBAAiBf,KAAK,CAACgB,eAAe,IAAI