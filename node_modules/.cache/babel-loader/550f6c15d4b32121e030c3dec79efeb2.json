{"ast":null,"code":"// import store from \"@/store/index\";\nimport $ from \"jquery\";\nimport axios from \"axios\";\nexport default {\n  name: \"MyHeader\",\n\n  data() {\n    return {\n      cart: [],\n      amount: 0,\n      memberId: 1\n    };\n  },\n\n  mounted() {// axios\n    //   .get(\n    //     `http://localhost:8080/IIIFinalServlet_war_exploded/CartServlet?pdaction=cartCheckOut2`\n    //   )\n    //   .then((res) => {\n    //     this.cart = res.data;\n    //   });\n  },\n\n  methods: {\n    dropdown() {\n      $(\".top-cart-content\").slideDown();\n    },\n\n    up() {\n      $(\".top-cart-content\").slideUp();\n    },\n\n    getCart() {\n      axios.get(`http://localhost:8081/api/cart/checkout`).then(res => {\n        this.cart = res.data;\n      });\n      axios.post(\"http://localhost:8081/dashboardGetMemberId\").then(res => {\n        this.memberId = res.data;\n      }).catch(error => console.log(error));\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgLA;AACA,OAAOA,CAAP,MAAc,QAAd;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EADD;MAELC,MAAM,EAAE,CAFH;MAGLC,QAAQ,EAAE;IAHL,CAAP;EAKD,CARY;;EASbC,OAAO,GAAG,CACR;IACA;IACA;IACA;IACA;IACA;IACA;EACD,CAjBY;;EAmBbC,OAAO,EAAE;IACPC,QAAQ,GAAG;MACTT,CAAC,CAAC,mBAAD,CAAD,CAAuBU,SAAvB;IACD,CAHM;;IAIPC,EAAE,GAAG;MACHX,CAAC,CAAC,mBAAD,CAAD,CAAuBY,OAAvB;IACD,CANM;;IAOPC,OAAO,GAAG;MACRZ,KAAK,CAACa,GAAN,CAAW,yCAAX,EAAqDC,IAArD,CAA2DC,GAAD,IAAS;QACjE,KAAKZ,IAAL,GAAYY,GAAG,CAACb,IAAhB;MACD,CAFD;MAGAF,KAAI,CACDgB,IADH,CACQ,4CADR,EAEGF,IAFH,CAESC,GAAD,IAAS;QACb,KAAKV,QAAL,GAAgBU,GAAG,CAACb,IAApB;MACD,CAJH,EAKGe,KALH,CAKUC,KAAD,IAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CALpB;IAMD;;EAjBM;AAnBI,CAAf","names":["$","axios","name","data","cart","amount","memberId","mounted","methods","dropdown","slideDown","up","slideUp","getCart","get","then","res","post","catch","error","console","log"],"sourceRoot":"","sources":["/Users/hank/Desktop/Vue/Vue3/hello-world/src/components/MyHeader.vue"],"sourcesContent":["<template>\n  <header>\n    <div class=\"header-container\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <!-- Search-col -->\n          <div class=\"col-lg-4 col-xs-12 col-sm-4 search-section hidden-xs\">\n            <div class=\"search-box pull-left\">\n              <form\n                action=\"cat\"\n                method=\"POST\"\n                id=\"search_mini_form\"\n                name=\"Categories\"\n              >\n                <input\n                  type=\"text\"\n                  name=\"search\"\n                  value=\"\"\n                  placeholder=\"Search\"\n                  class=\"searchbox\"\n                  id=\"search\"\n                  autocomplete=\"off\"\n                /><button type=\"button\" class=\"search-btn-bg\">\n                  <span class=\"glyphicon glyphicon-search\"></span>&nbsp;\n                </button>\n              </form>\n            </div>\n          </div>\n          <!-- End Search-col -->\n          <div class=\"col-lg-4 col-sm-4 col-xs-12 logo-block\">\n            <!-- Header Logo -->\n            <div class=\"logo\">\n              <a href=\"index.html\">\n                <router-link\n                  :to=\"{\n                    name: 'home',\n                  }\"\n                >\n                  <img src=\"../assets/images/5.png\" alt=\"Kabrio logo\" />\n                </router-link>\n              </a>\n            </div>\n            <!-- End Header Logo -->\n          </div>\n          <div class=\"col-lg-4 col-sm-4 col-xs-12 toplinks-right\">\n            <!-- <div class=\"mm-toggle-wrap\">\n              <div class=\"mm-toggle\">\n                <i class=\"fa fa-align-justify\"></i\n                ><span class=\"mm-label\">Menu</span>\n              </div>\n            </div> -->\n\n            <div\n              class=\"top-cart-contain\"\n              @click=\"dropdown(), getCart()\"\n              @mouseenter=\"dropdown(), getCart()\"\n            >\n              <!-- Top Cart -->\n              <div class=\"mini-cart\">\n                <div class=\"basket\">\n                  <a\n                    ><span>{{\n                      cart == \"購物車內目前沒有商品!\" ? \"0\" : cart.length\n                    }}</span></a\n                  >\n                </div>\n                <div>\n                  <div class=\"top-cart-content\" @mouseleave=\"up()\">\n                    <div class=\"block-subtitle\">\n                      <div class=\"top-subtotal\">\n                        {{\n                          cart == \"購物車內目前沒有商品!\" ? \"0\" : cart.length\n                        }}\n                        items,\n\n                        <span class=\"price\">${{ amount }}</span>\n                      </div>\n                    </div>\n                    <ul\n                      v-if=\"cart != '購物車內目前沒有商品!'\"\n                      class=\"mini-products-list\"\n                      id=\"cart-sidebar\"\n                    >\n                      <li v-for=\"item in cart\" :key=\"item\" class=\"item\">\n                        <div class=\"item-inner\">\n                          <a\n                            class=\"product-image\"\n                            title=\"Lorem ipsum dolor\"\n                            href=\"#l\"\n                            ><img\n                              alt=\"Lorem ipsum dolor\"\n                              :src=\"item.productImg\"\n                          /></a>\n                          <div class=\"product-details\">\n                            <div class=\"access\">\n                              <a\n                                class=\"btn-remove1\"\n                                title=\"Remove This Item\"\n                                :href=\"\n                                  'http://localhost:8080/IIIFinalServlet_war_exploded/CartServlet?pdaction=removeSingleProducts&editProductId=' +\n                                  item.productId\n                                \"\n                                >Remove</a\n                              ><a class=\"btn-edit\" title=\"Edit item\" href=\"#\"\n                                ><i class=\"icon-pencil\"></i\n                                ><span class=\"hidden\">Edit item</span></a\n                              >\n                            </div>\n                            <p class=\"product-name\">\n                              <a href=\"#\">{{ item.productName }}</a>\n                            </p>\n                            <strong>{{ item.qty }}</strong\n                            >x\n                            <span class=\"price\">{{ item.singleTotal }}</span>\n                          </div>\n                        </div>\n                      </li>\n                    </ul>\n                    <div class=\"actions\">\n                      <router-link\n                        :to=\"{\n                          name: memberId > 0 ? 'checkout' : 'login',\n                        }\"\n                      >\n                        <button\n                          class=\"btn-checkout\"\n                          title=\"Checkout\"\n                          type=\"button\"\n                        >\n                          <span>結帳</span>\n                        </button>\n                      </router-link>\n                    </div>\n                  </div>\n                </div>\n              </div>\n              <!-- End Top Cart -->\n            </div>\n            <!-- Header Top Links -->\n            <div class=\"toplinks\">\n              <div class=\"click-nav\">\n                <div class=\"no-js\">\n                  <a title=\"Company\" class=\"clicker\"\n                    ><span class=\"icon-account\"\n                      ><img\n                        src=\"../assets/images/icon-account.jpg\"\n                        alt=\"icon-account\" /></span\n                  ></a>\n                  <div class=\"top-links\">\n                    <ul class=\"links\">\n                      <li>\n                        <router-link\n                          :to=\"{\n                            name: 'login',\n                          }\"\n                        >\n                          登入\n                        </router-link>\n                      </li>\n                      <li><a href=\"login.html\">我的帳戶</a></li>\n                    </ul>\n                  </div>\n                </div>\n              </div>\n            </div>\n            <!-- End Header Top Links -->\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- <template v-for=\"item in cart\" :key=\"item.id\"> -->\n    <!-- </template> -->\n  </header>\n</template>\n\n<script>\n// import store from \"@/store/index\";\nimport $ from \"jquery\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"MyHeader\",\n  data() {\n    return {\n      cart: [],\n      amount: 0,\n      memberId: 1,\n    };\n  },\n  mounted() {\n    // axios\n    //   .get(\n    //     `http://localhost:8080/IIIFinalServlet_war_exploded/CartServlet?pdaction=cartCheckOut2`\n    //   )\n    //   .then((res) => {\n    //     this.cart = res.data;\n    //   });\n  },\n\n  methods: {\n    dropdown() {\n      $(\".top-cart-content\").slideDown();\n    },\n    up() {\n      $(\".top-cart-content\").slideUp();\n    },\n    getCart() {\n      axios.get(`http://localhost:8081/api/cart/checkout`).then((res) => {\n        this.cart = res.data;\n      });\n      axios\n        .post(\"http://localhost:8081/dashboardGetMemberId\")\n        .then((res) => {\n          this.memberId = res.data;\n        })\n        .catch((error) => console.log(error));\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.cart {\n  width: 300px !important;\n  padding: 0 !important;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}