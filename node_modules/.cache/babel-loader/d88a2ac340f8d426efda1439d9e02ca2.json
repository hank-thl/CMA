{"ast":null,"code":"import store from \"@/store/index\";\nimport axios from \"axios\";\nimport $ from \"jquery\";\nexport default {\n  name: \"MyProduct\",\n\n  data() {\n    return {\n      myIndex: 0,\n      qty: 1\n    };\n  },\n\n  mounted() {\n    store.dispatch(\"GETPRODUCTS\");\n  },\n\n  computed: {\n    id() {\n      return this.$route.params.id;\n    },\n\n    product() {\n      return store.state.products.filter(item => {\n        return item.productId == this.id;\n      });\n    }\n\n  },\n  methods: {\n    addToCart() {\n      console.log(\"ppppp\");\n      axios.get(`http://localhost:8081/api/cart/add/${this.id}/qty/${this.qty}`).then(response => console.log(response)).catch(error => console.log(error));\n    },\n\n    changeImage(no) {\n      let temp;\n      temp = $(`#productImg${no}`).prop(\"src\");\n      $(`#productImg${no}`).prop(\"src\", $(\"#productImg1\").prop(\"src\"));\n      $(\"#productImg1\").prop(\"src\", temp);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsqBA,OAAOA,KAAP,MAAkB,eAAlB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,OAAOC,CAAP,MAAc,QAAd;AAEA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,CADJ;MAELC,GAAG,EAAE;IAFA,CAAP;EAID,CAPY;;EAQbC,OAAO,GAAG;IACRP,KAAK,CAACQ,QAAN,CAAe,aAAf;EACD,CAVY;;EAWbC,QAAQ,EAAE;IACRC,EAAE,GAAG;MACH,OAAO,KAAKC,MAAL,CAAYC,MAAZ,CAAmBF,EAA1B;IACD,CAHO;;IAIRG,OAAO,GAAG;MACR,OAAOb,KAAK,CAACc,KAAN,CAAYC,QAAZ,CAAqBC,MAArB,CAA6BC,IAAD,IAAU;QAC3C,OAAOA,IAAI,CAACC,SAAL,IAAkB,KAAKR,EAA9B;MACD,CAFM,CAAP;IAGD;;EARO,CAXG;EAqBbS,OAAO,EAAE;IACPC,SAAS,GAAG;MACVC,OAAO,CAACC,GAAR,CAAY,OAAZ;MACArB,KAAI,CACDsB,GADH,CACQ,sCAAqC,KAAKb,EAAG,QAAO,KAAKJ,GAAI,EADrE,EAEGkB,IAFH,CAESC,QAAD,IAAcJ,OAAO,CAACC,GAAR,CAAYG,QAAZ,CAFtB,EAGGC,KAHH,CAGUC,KAAD,IAAWN,OAAO,CAACC,GAAR,CAAYK,KAAZ,CAHpB;IAID,CAPM;;IAQPC,WAAW,CAACC,EAAD,EAAK;MACd,IAAIC,IAAJ;MACAA,IAAG,GAAI5B,CAAC,CAAE,cAAa2B,EAAG,EAAlB,CAAD,CAAsBE,IAAtB,CAA2B,KAA3B,CAAP;MACA7B,CAAC,CAAE,cAAa2B,EAAG,EAAlB,CAAD,CAAsBE,IAAtB,CAA2B,KAA3B,EAAkC7B,CAAC,CAAC,cAAD,CAAD,C